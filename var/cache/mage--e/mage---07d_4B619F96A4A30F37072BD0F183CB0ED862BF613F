<script type="text/javascript">
//<![CDATA[
jQuery(document).ready(function($) {
    $('#sns_lider86151478614901 div.products-grid').owlCarousel({
        itemsCustom: [
            [0, 1],
            [480,2],
            [641,2]
        ],
                        pagination: false,
        slideSpeed : 800,
        addClassActive: true,
        afterAction: function (e) {
            if(this.$owlItems.length > this.options.items){
                $('#sns_lider86151478614901 .navslider').show();
            }else{
                $('#sns_lider86151478614901 .navslider').hide();
            }
        }
        //scrollPerPage: true,
    });
    $('#sns_lider86151478614901 .navslider .prev').on('click', function(e){
        e.preventDefault();
        $('#sns_lider86151478614901 div.products-grid').trigger('owl.prev');
    });
    $('#sns_lider86151478614901 .navslider .next').on('click', function(e){
        e.preventDefault();
        $('#sns_lider86151478614901 div.products-grid').trigger('owl.next');
    });
});
//]]>
</script>
