<script type="text/javascript">
//<![CDATA[
jQuery(document).ready(function($) {
    $('#sns_lider250821478532400 div.products-grid').owlCarousel({
        itemsCustom: [
            [0, 1], 
			[480,2], 
			[768,2], 
			[992, 2], 
			[1200, 2] 
        ],
                        pagination: false,
        slideSpeed : 800,
        addClassActive: true,
        afterAction: function (e) {
            if(this.$owlItems.length > this.options.items){
                $('#sns_lider250821478532400 .navslider').show();
            }else{
                $('#sns_lider250821478532400 .navslider').hide();
            }
        }
        //scrollPerPage: true,
    });
    $('#sns_lider250821478532400 .navslider .prev').on('click', function(e){
        e.preventDefault();
        $('#sns_lider250821478532400 div.products-grid').trigger('owl.prev');
    });
    $('#sns_lider250821478532400 .navslider .next').on('click', function(e){
        e.preventDefault();
        $('#sns_lider250821478532400 div.products-grid').trigger('owl.next');
    });
});
//]]>
</script>
